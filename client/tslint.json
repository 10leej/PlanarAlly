{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "array-type": [
      true,
      "array"
    ],
    "curly": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "member-access": false,
    "member-ordering": false,
    "no-angle-bracket-type-assertion": false,
    "no-bitwise": false,
    "no-console": false,
    "no-empty": false,
    "object-literal-sort-keys": false
  }
}