{
    "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
    ],
    "rules": {
      "object-literal-sort-keys": false,
      "no-angle-bracket-type-assertion": false,
      "curly": false,
      "no-bitwise": false,
      "member-access": false,
      "member-ordering": false,
      "interface-name": false,
      "no-console": false,
      "array-type": [true, "array"],
      "no-empty": false,
      "max-classes-per-file": false
    }
  }